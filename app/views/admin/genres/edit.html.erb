<div class="container">
  <div class="row">
    <div class="">
      <h3 class="title">ジャンル編集</h3>
    </div>
      <%= form_with model: @genre,url: admin_genre_path(@genre) do |f| %>
        <h5>ジャンル名</h5>
          <%= f.text_field :name %>
          <%= f.submit "変更を保存する",class: "btn btn-success" %>
          <%= link_to "戻る",admin_genres_path,class: "btn btn-private" %>
      <% end %>
  </div>
</div>